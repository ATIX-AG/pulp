import sys
import os

# quick check to see if you are a super-user.
if os.getuid() != 0:
    sys.stderr.write('ERROR: must be root to execute\n')
    sys.exit(8)

from pulptools import pulpcli


if __name__ == "__main__":
    pulpcli.PulpCore().main()

